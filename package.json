{
  "name": "at-models-ts",
  "description": "AT shared models for TypeScript projects",
  "version": "0.0.0",
  "main": "lib/index.ts",
  "private": false,
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts src tests"
  },
  "publishConfig": {
